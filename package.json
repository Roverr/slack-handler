{
  "name": "slack-handler",
  "version": "1.0.0",
  "description": "This is a slack message service inspired by slack-node.",
  "main": "index.js",
  "config": {
    "MOCHA_OPTS": "--require babel-polyfill --compilers js:babel-core/register --env test"
  },
  "scripts": {
    "clean": "rimraf ./lib",
    "build": "npm run clean && babel src -d lib",
    "start": "babel-node ./src/index",
    "test": "NODE_ENV=test mocha $npm_package_config_MOCHA_OPTS 'tests/**/*-test.js'",
    "lint": "eslint test"
  },
  "keywords": [
    "slack"
  ],
  "author": "Imre 'Rover' Racz",
  "license": "ISC",
  "dependencies": {
    "lodash": "^4.6.1",
    "requestretry": "^1.6.0"
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-core": "^6.7.4",
    "babel-eslint": "^6.0.0",
    "babel-plugin-transform-async-to-generator": "^6.7.4",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "chai": "^3.5.0",
    "chai-spies": "^0.7.1",
    "chance": "^1.0.1",
    "eslint": "^1.10.3",
    "eslint-config-codingsans": "^1.2.0",
    "mocha": "^2.4.5",
    "restify": "^4.0.4",
    "rimraf": "^2.5.2",
    "supertest": "^1.2.0",
    "supertest-as-promised": "^3.1.0"
  }
}
